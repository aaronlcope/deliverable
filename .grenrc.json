{
    "dataSource": "commits",
    "prefix": "",
    "includeMessages": "all",
    "changelogFilename": "CHANGELOG.md",
    "ignoreIssuesWith": [
        "wontfix",
        "duplicate"
    ],
    "groupBy": {
        "Enhancements:": ["enhancement", "internal"],
        "Bug Fixes:": ["bug"],
        "Documentation:": ["documentation"]
    }
}